<%= hidden_field_tag :direction, params[:direction] %>
<%= hidden_field_tag :sort, params[:sort] %>

<table class="table table-hover">
  <thead>
  <tr class="alert-success">
    <th style="width: 150px;"><%= t "dmhangsx.stt" %></th>
    <th> <%= t('dmhangsx.ten') %></th>
    <th><%=  t('dmhangsx.dmnhomnhangsx_id') %></th>
    <td style="width: 60px;">&nbsp;</td>
  </tr>
  </thead>
  <tbody>
  <% @dmhangsxes.each do |dmhangsx| %>
      <tr>
        <td><%= dmhangsx.stt %></td>
        <td><%= dmhangsx.ten %></td>
        <td><%= dmhangsx.dmnhomhangsx.ten %></td>
        <td><%= link_to edit_danhmuc_dmhangsx_path(dmhangsx), data: {remote: true}, :title => t('edit') do %>
              <i class="glyphicon glyphicon-edit"></i>
          <% end %>
          <%= link_to danhmuc_dmhangsx_delete_path(dmhangsx), data: {remote: true}, :title => t('delete') do %>
              <i class="glyphicon glyphicon-trash"></i>
          <% end %>
        </td>
      </tr>
  <% end %>
  </tbody>
  <tfoot id="table_paginate">
  <tr class="text-center">
    <td colspan="4">
      <%= will_paginate @dmhangsxes, renderer: BootstrapPagination::Rails %>
    </td>
  </tr>
  </tfoot>
</table>