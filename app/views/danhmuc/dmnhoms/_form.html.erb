<%= form_for [:danhmuc ,@dmnhom], :data => {remote: true}, :html => {:class => "form-horizontal", role: "form"} do |f| %>
    <div class="modal-body">
      <div class="row">
        <ul class="errors"></ul>
      </div>
      <div class="row">
        <div class="col-lg-3"></div>
        <div class="col-lg-6">
          <div class="form-group">
            <label for="stt"><%= t "dmnhom.stt" %></label>
            <%= f.text_field :stt, class: "form-control" %>
          </div>
          <div class="form-group">
            <label for="ten"><%= t "dmnhom.ten" %></label>
            <%= f.text_field :ten, class: "form-control" %>
          </div>
          <div class="form-group">
            <label for="dmnhombc_id"><%= t "dmnhom.dmnhombc_id" %></label>
            <%= f.select(:dmnhombc_id, Dmnhombc.all.collect { |c| [c.ten, c.id] }, {:prompt => t('dmnhom.promt')}, {:class => "form-control"}) %>
          </div>
          <div class="form-group">
            <label for="dmnhomin_id"><%= t "dmnhom.dmnhomin_id" %></label>
            <%= f.select(:dmnhomin_id, Dmnhomin.all.collect { |c| [c.ten, c.id] }, {:prompt => t('dmnhom.promt')}, {:class => "form-control"}) %>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="submit" class="btn btn-primary">
          <i class="glyphicon glyphicon-floppy-save"></i> <%= t "save" %>
        </button>
        <%= link_to danhmuc_dmnhoms_path, class: "btn btn-default", data: {dismiss: "modal"} do %>
            <i class="glyphicon glyphicon-chevron-left"></i> <%= t "back" %>
        <% end %>
      </div>
      <div class="col-lg-3"></div>
    </div>
<% end %>