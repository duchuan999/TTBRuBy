<%= form_for [:danhmuc ,@dmttb], :data => {remote: true}, :html => {:class => "form-horizontal", role: "form"} do |f| %>
    <div class="modal-body">
      <div class="row">
        <ul class="errors"></ul>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-12">
          <div class="col-lg-2">
            <label for="stt"><%= t "dmttb.stt" %></label>
            <%= f.number_field :stt, class: "form-control" %>
          </div>
          <div class="col-lg-2">
            <label for="ma"><%= t "dmttb.ma" %></label>
            <%= f.text_field :ma, class: "form-control" %>
          </div>
          <div class="col-lg-5">
            <label for="ten"><%= t "dmttb.ten" %></label>
            <%= f.text_field :ten, class: "form-control" %>
          </div>
          <div class="col-lg-3">
            <label for="model"><%= t "dmttb.model" %></label>
            <%= f.text_field :model, class: "form-control" %>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-12">
          <div class="col-lg-3">
            <label for="dmdvt_id"><%= t "dmttb.dmdvt_id" %></label>
            <%= f.select(:dmdvt_id, Dmdvt.all.collect { |c| [c.ten, c.id] }, {:prompt => t('dmttb.promtdvt')}, {:class => "form-control"}) %>

          </div>
          <div class="col-lg-3">
            <label for="hamluong"><%= t "dmttb.hamluong" %></label>
            <%= f.text_field :hamluong, class: "form-control" %>
          </div>
          <div class="col-lg-3">
            <label for="congsuat"><%= t "dmttb.congsuat" %></label>
            <%= f.text_field :congsuat, class: "form-control" %>
          </div>
          <div class="col-lg-3">
            <label for="tylekh"><%= t "dmttb.tylekh" %></label>
            <div class="input-group"><%= f.text_field :tylekh, class: "form-control" %> <span class="input-group-addon"><%= t "dmttb.loaikh" %></span>
              </div>
          </div>
        </div>
      </div>

      <div class="row">
        <div class="col-xs-12 col-md-12">
          <div class="col-lg-4">
            <label for="dmdvt_id"><%= t "dmttb.dmnhom_id" %></label>
            <%= f.select(:dmnhom_id, Dmnhom.all.collect { |c| [c.ten, c.id] }, {:prompt => t('dmttb.promt')}, {:class => "form-control"}) %>

          </div>
          <div class="col-lg-4">
            <label for="dmhangsx_id"><%= t "dmttb.dmhangsx_id" %></label>
            <%= f.select(:dmhangsx_id, Dmhangsx.all.collect { |c| [c.ten, c.id] }, {:prompt => t('dmttb.promtdvt')}, {:class => "form-control"}) %>
          </div>
          <div class="col-lg-4">
            <label for="dmnuocsx_id"><%= t "dmttb.dmnuocsx_id" %></label>
            <%= f.select(:dmnuocsx_id, Dmnuocsx.all.collect { |c| [c.ten, c.id] }, {:prompt => t('dmttb.promtdvt')}, {:class => "form-control"}) %>
          </div>
        </div>

      </div>
      <div class="row">
        <div class="col-xs-12 col-md-12">
          <div class="col-lg-4">
            <label for="dmnhombc_id"><%= t "dmttb.dmnhombc_id" %></label>
            <%= f.select(:dmnhombc_id, Dmnhombc.all.collect { |c| [c.ten, c.id] }, {:prompt => t('dmttb.promt')}, {:class => "form-control"}) %>

          </div>
          <div class="col-lg-4">
            <label for="dmnhomin_id"><%= t "dmttb.dmnhomin_id" %></label>
            <%= f.select(:dmnhomin_id, Dmnhomin.all.collect { |c| [c.ten, c.id] }, {:prompt => t('dmttb.promt')}, {:class => "form-control"}) %>
          </div>
          <div class="col-lg-4">
            <label for="sodk"><%= t "dmttb.sodk" %></label>
            <%= f.text_field :sodk, class: "form-control" %>
          </div>
        </div>

      </div>

      <div class="row">
        <div class="col-xs-12 col-md-12">
          <div class="col-lg-12">
            <label for="hinh"><%= t "dmttb.hinh" %></label>
            <div class="input-group"> <%= f.text_field :hinh, class: "form-control" %>
            <span class="input-group-btn"> <button class="btn btn-default" type="button">...</button></span></div>
          </div>
        </div>
      </div>

      <div class="modal-footer">
        <button type="submit" class="btn btn-primary">
          <i class="glyphicon glyphicon-floppy-save"></i> <%= t "save" %>
        </button>
        <%= link_to danhmuc_dmttbs_path, class: "btn btn-default", data: {dismiss: "modal"} do %>
            <i class="glyphicon glyphicon-chevron-left"></i> <%= t "back" %>
        <% end %>
      </div>
      <div class="col-lg-3"></div>
    </div>
<% end %>