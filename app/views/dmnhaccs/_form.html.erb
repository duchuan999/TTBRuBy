<%= form_for @dmnhacc, :data => {remote: true}, :html => {:class => "form-horizontal", role: "form"} do |f| %>
    <div class="modal-body">
      <div class="row">
        <ul class="errors"></ul>
      </div>
      <div class="row">
        <div class="col-xs-12 col-md-12">
          <div class="col-lg-3">
            <label for="stt"><%= t "dmnhacc.stt" %></label>
            <%= f.text_field :stt, class: "form-control" %>
          </div>
          <div class="col-lg-3">
            <label for="ma"><%= t "dmnhacc.ma" %></label>
            <%= f.text_field :ma, class: "form-control" %>
          </div>
          <div class="col-lg-6">
            <label for="ten"><%= t "dmnhacc.ten" %></label>
            <%= f.text_field :ten, class: "form-control" %>
          </div>

        </div>
      </div>
      <!--end row-->
      <div class="row">
        <div class="col-xs-12 col-md-12">

          <div class="col-lg-9">
            <label for="diachi"><%= t "dmnhacc.diachi" %></label>
            <%= f.text_field :diachi, class: "form-control" %>
          </div>
          <div class="col-lg-3">
            <label for="dt"><%= t "dmnhacc.dt" %></label>
            <%= f.text_field :dt, class: "form-control" %>
          </div>
        </div>

      </div>
      <!--end row-->
      <div class="row">
        <div class="col-xs-12 col-md-12">

          <div class="col-lg-4">
            <label for="mst"><%= t "dmnhacc.mst" %></label>
            <%= f.text_field :masothue, class: "form-control" %>
          </div>
          <div class="col-lg-4">
            <label for="stk"><%= t "dmnhacc.stk" %></label>
            <%= f.text_field :sotk, class: "form-control" %>
          </div>
          <div class="col-lg-4">
            <label for="daidien"><%= t "dmnhacc.daidien" %></label>
            <%= f.text_field :daidien, class: "form-control" %>
          </div>

        </div>
      </div>
      <!--end row-->
      <div class="row">
        <div class="col-xs-12 col-md-12">
          <div class="col-lg-12">
            <label for="vungdialy_id"><%= t "dmnhacc.dmnhomnhacc_id" %></label>
            <%= f.select(:dmnhomnhacc_id, Dmnhomnhacc.all.collect { |c| [c.ten, c.id] }, {:prompt => t('dmhangsx.promt')}, {:class => "form-control"}) %>
          </div>
        </div>
      </div>

      <div class="modal-footer">
        <button type="submit" class="btn btn-primary">
          <i class="glyphicon glyphicon-floppy-save"></i> <%= t "save" %>
        </button>
        <%= link_to dmnhaccs_path, class: "btn btn-default", data: {dismiss: "modal"} do %>
            <i class="glyphicon glyphicon-chevron-left"></i> <%= t "back" %>
        <% end %>
      </div>
      <div class="col-lg-3"></div>
    </div>
<% end %>