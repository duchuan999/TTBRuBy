<!DOCTYPE html>
<html>
<head>
  <title>TTBRuBy</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<header>
  <%= render 'layouts/navigate' %>
</header>
<div class="container">
  <div class="row">
    <div class="col-lg-3">
      <%= render 'sidebar' %>
    </div>
    <div class="col-lg-9">
      <%= yield %>
    </div>
  </div>
</div>
<footer>
  <%= render 'layouts/footer' %>
</footer>
</body>
</html>
